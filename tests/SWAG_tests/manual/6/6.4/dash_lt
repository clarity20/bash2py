dash_a="missing";
dash_ot=ot
dash_ef=ef

if [ dash_ot -lt dash_ef ]
then
echo "true";
else
echo "false";
fi

if [ dash_ot -lt $dash_ef ]
then
echo "true";
else
echo "false";
fi

if [ $dash_ot -lt dash_ef ]
then
echo "true";
else
echo "false";
fi

if [ $dash_ot -lt $dash_ef ]
then
echo "true";
else
echo "false";
fi

if [ dash_ot -lt dash_a ]
then
echo "true";
else
echo "false";
fi

if [ dash_a -lt dash_ot ]
then
echo "true";
else
echo "false";
fi

