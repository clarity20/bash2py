     ;; #(
  *) :

      # Both programs failed to compile - this probably means getgroups
      # wasn't declared at all.  Use 'int', as this is probably a very
      # old system where the type _would have been_ int.
      ac_cv_type_getgroups=int
     ;;
esac
   ;;
esac
fi
 ;;
esac
fi
